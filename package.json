{
  "name": "cypress-automation-framework",
  "version": "1.0.0",
  "description": "Cypress Automation Framework",
  "main": "index.js",
  "scripts": {
    "triggerAllTests-headless": "NO_COLOR=1 npx cypress run",
    "triggerAllTests-headed": "NO_COLOR=1 npx cypress run --headed",
    "triggerAllTests-chrome": "NO_COLOR=1 npx cypress run --browser chrome",
    "triggerAllTests-dashboard": "NO_COLOR=1 npx cypress run --record --key 3bbc3e4b-e370-4579-bbd6-62778fc2c677",
    "triggerAllTests-webdriveruni": "NO_COLOR=1 npx cypress run --spec cypress/e2e/webdriver-uni/*",
    "triggerAllTests-autoTestStore": "NO_COLOR=1 npx cypress run --spec cypress/e2e/automation-test-store/*",
    "triggerAllTests-autoTestStore-dashboard": "NO_COLOR=1 npx cypress run --spec cypress/e2e/automation-test-store/* --record --key 3bbc3e4b-e370-4579-bbd6-62778fc2c677 --parallel",
    "junit-merge": "NO_COLOR=1 npx junit-merge -d cypress/results/junit -o cypress/results/junit/results.xml",
    "delete-junit-report": "NO_COLOR=1 rm -rf cypress/results/junit/results.xml",
    "delete-results": "NO_COLOR=1 rm -rf cypress/results/* || true",
    "mochawesome-merge": "NO_COLOR=1 npx mochawesome-merge cypress/results/mochawesome/*.json > mochawesome.json && npx marge mochawesome.json",
    "delete-mochawesome-report": "NO_COLOR=1 rm -rf mochawesome-report/* || true",
    "cypress-regression-pack": "NO_COLOR=1 npm run delete-results && npm run delete-mochawesome-report && npm run triggerAllTests-headless && npm run mochawesome-merge",
    "triggerAllTests-staging": "NO_COLOR=1 npx cypress run --env configFile=staging",
    "cypress-multi-browser": "NO_COLOR=1 npm run triggerAllTests-autoTestStore -- --browser electron --headed & npm run triggerAllTests-autoTestStore -- --browser edge --headed"
  },
  "author": "Dragan",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.6.0",
    "cypress-multi-reporters": "^1.6.2",
    "cypress-xpath": "^2.0.1",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0"
  }
}
